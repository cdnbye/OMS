{
  "name": "oss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "deploy-github": "sh deploy_github.sh",
    "deploy-gitee": "sh deploy.sh",
    "deploy-test": "sh deploy-prerelease.sh",
    "git-push": "git add . && git commit -m 'u' &&  git push",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode production",
    "build-prerelease": "vue-cli-service build --mode test",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cache-loader": "^4.1.0",
    "clipboard": "^2.0.4",
    "crisp-sdk-web": "^1.0.13",
    "dayjs": "^1.11.5",
    "echarts": "^4.2.0-rc.1",
    "element-ui": "2.15.8",
    "js-base64": "^2.4.9",
    "js-cookie": "^2.2.0",
    "js-sha256": "^0.9.0",
    "normalize.css": "^8.0.0",
    "reconnecting-websocket": "^4.4.0",
    "sass-loader": "^7.1.0",
    "screenfull": "4.2.0",
    "svg-sprite-loader": "^3.9.2",
    "url-toolkit": "^2.2.5",
    "vue": "^2.5.17",
    "vue-count-to": "^1.0.13",
    "vue-flag-list": "^1.0.4",
    "vue-i18n": "^8.1.0",
    "vue-json-excel": "^0.3.0",
    "vue-json-pretty": "^1.9.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "babel-plugin-component": "^1.1.1",
    "sass": "^1.54.5",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-component-is": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
